platform :ios, '11.0'

inhibit_all_warnings!

target 'SmartSleep' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SmartSleep
  pod 'KeychainAccess', '~> 3.1.2'
  pod 'RxSwift', '~> 4.4.1'
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'

end
